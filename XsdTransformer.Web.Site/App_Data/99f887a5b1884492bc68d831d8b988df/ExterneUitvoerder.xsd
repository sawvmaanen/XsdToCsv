	<xsd:import namespace="http://cdm.stedin.net/communicatie/typen/algemeen/header/1.0" schemaLocation="header.xsd"/>
	<xsd:import namespace="http://www.stedin.net/infrastructuur/dsl/cdm/1.0" schemaLocation="DSLCDM.xsd"/>
	<xsd:import namespace="http://cdm.stedin.net/structuren/resultaat/1" schemaLocation="resultaat.xsd"/>
	<xsd:element name="VerzendenWerkordersRequest">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="header" type="head:header"></xsd:element>
				<xsd:element ref="cdm:Werkorder" minOccurs="1" maxOccurs="1"></xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="VerzendenWerkordersResponse">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="header" type="head:header"></xsd:element>
				<xsd:element name="resultaat" type="res:resultaat"></xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	
	
	<xsd:element name="AnnulerenWerkordersRequest">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="Werkorders">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Werkorder" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="Werkordernummer" type="xsd:string"/>
										<xsd:element name="Volgnummer" type="xsd:integer"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="AnnulerenWerkordersResponse" type="tns:ResponseType"/>
	<xsd:element name="WijzigenWerkordersRequest">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="Werkorders">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Werkorder" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="Werkordernummer" type="xsd:string"/>
										<xsd:element name="Volgnummer" type="xsd:integer"/>
										<xsd:element name="TijdslotStarttijd" type="xsd:dateTime" minOccurs="0"/>
										<xsd:element name="TijdslotEindtijd" type="xsd:dateTime" minOccurs="0"/>
										<xsd:element name="Klantnaam" type="xsd:string" minOccurs="0"/>
										<xsd:element name="KlantTelefoonnummer" type="xsd:string" minOccurs="0"/>
										<xsd:element name="GewensteCapaciteitE" type="tns:fysiekeCapaciteit" minOccurs="0"/>
										<xsd:element name="GewensteCapaciteitG" type="tns:fysiekeCapaciteit" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="WijzigenWerkordersResponse" type="tns:ResponseType"/>
