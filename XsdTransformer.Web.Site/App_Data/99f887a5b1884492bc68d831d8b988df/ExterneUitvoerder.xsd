	<xsd:complexType name="ResponseType">
		<xsd:sequence>
			<xsd:element name="Resultaat">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Code" type="xsd:string"/>
						<xsd:element name="Meldingen">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="Melding" maxOccurs="unbounded">
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="Code" type="xsd:string"/>
												<xsd:element name="Omschrijving" type="xsd:string"/>
												<xsd:element name="Tekst" type="xsd:string"/>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	
