			<xsd:enumeration value="G10"/>
			<xsd:enumeration value="G16"/>
			<xsd:enumeration value="G25"/>
			<xsd:enumeration value="G40"/>
			<xsd:enumeration value="G65"/>
			<xsd:enumeration value="G100"/>
			<xsd:enumeration value="G160"/>
			<xsd:enumeration value="G250"/>
