<?xml version="1.0" encoding="UTF-8"?>
<xs:schema	xmlns:xs="http://www.w3.org/2001/XMLSchema"
			targetNamespace="http://cdm.stedinmeetbedrijf.nl/attributen/verbruik/1.0" 
			xmlns:tns="http://cdm.stedinmeetbedrijf.nl/attributen/verbruik/1.0"
			elementFormDefault="qualified">
    <!--
    DEFINITION  : Vastgestelde verbruik direct uit de meting opgemaakt in de verbruikperiode.
    TYPE        : numeric(1,15)
    FORMAT      : [0-9]{1,15}
    -->
    <xs:simpleType name="verbruik">
        <xs:restriction base="xs:integer">
        	<xs:pattern value="[0-9]{1,15}" />
        </xs:restriction>
    </xs:simpleType>
</xs:schema>