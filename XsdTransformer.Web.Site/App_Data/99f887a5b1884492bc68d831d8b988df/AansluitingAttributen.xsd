            <xs:enumeration value="VGS"/>
            <xs:enumeration value="ALC"/>
            <xs:enumeration value="CTF"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="soortAansluitingTA">
		<xs:sequence>
			<xs:element name="soortAansluiting" type="tns:soortAansluiting"  minOccurs="1" maxOccurs="1"/>
		    <xs:element name="startDatum" type="xs:date" minOccurs="1" maxOccurs="1"/>
		    <xs:element name="eindDatum" type="xs:date" minOccurs="1" maxOccurs="1"/>
		</xs:sequence>
    </xs:complexType>
    <!--
    DEFINITION  : Het verwachte jaarverbruik op de aansluiting.
    TYPE        : numeric(8)
    FORMAT      : 
    -->
